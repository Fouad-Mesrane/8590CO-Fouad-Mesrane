<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="ParaBank" verbose="1" >

    <listeners>
        <listener class-name="listeners.AnnotationTransformer"/>
    </listeners>

    <parameter name="browser" value="chrome"/>
    <parameter name="url" value="https://parabank.parasoft.com"/>

    <test name="tests">
        <classes>
            <class name="test.AuthenticationTest"/>
            <class name="test.RegistrationTest"/>
            <class name="test.LogoutTest"/>
            <class name="test.NewAccTest"/>
        </classes>
    </test>

</suite>